<div class="row">
<div  class="col-md-10 display-flex justify-content-center text-center">
  <h2 class="text-center">Welcome To Stash!!!</h2>
</div>
  <div class=welcome"col-md-6">
    <h2>Login</h2>

    <form class="form login-form">
      <div class="form-group">
        <label for="email-login">email:</label>
        <input class="form-input" type="text" autocomplete="username" id="email-login" />
      </div>
      <div class="form-group">
        <label for="password-login">password:</label>
        <input class="form-input" type="password" autocomplete="current-password"  id="password-login" />
      </div>
      <div class="form-group">
        <button class="btn btn-primary" type="submit">login</button>
      </div>
         <div class="form-group">
        <a href="/signup">New to Stash? Sign Up!</a>
      </div>
    </form>
  </div>
  <div class="col-md-6">


<script src="./js/login.js"></script>
